{% set className = category.name|lower|replace({' ': '-'}) %}
<div class="nominee {{ className }}">
    <h3 class="title title-{{ className }}">{{ category.name|upper }}</h3>
    <ul class="nominees-{{ className }}">
        {% for nominated in category.nominees %}
            {% set categoryName = category.name|lower|replace({'this': foo, ' ': "_"}) %}
            {{ include('nominees.html.twig',
                {
                    winner: category.winner,
                    categoryName,
                    nominated,
                    className
                }) }}
        {% endfor %}
    </ul>
</div>
